{
  "Config": {
    "DbHost": "DB_HOST",
    "DbName": "DB_NAME",
    "DbUser": "DB_USER",
    "DbPassword": "DB_PASSWORD",
    "Env": "ASPNETCORE_ENVIRONMENT",
    "InstantCoachStatusDefault": "New",
    "JwtAuthority": "JWT_AUTHORITY",
    "JwtAudience": "JWT_AUDIENCE"
  },
  "ElasticApm": {
    "TransactionSampleRate": 1.0,
    "MetricsInterval": "1s"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
